#:kivy 1.10.1


<AppActionBar>:
	email_label: _email_label
	ActionView:
		ActionPrevious:
			title: '[b]Spending Tracker[/b]'
			with_previous: False
			markup: True
			important: True
		ActionOverflow:
		ActionLabel:
			id: _email_label
			markup: True
		ActionButton:
			text: '[b]Logout[/b]'
			markup: True
			on_release: root.logout()
